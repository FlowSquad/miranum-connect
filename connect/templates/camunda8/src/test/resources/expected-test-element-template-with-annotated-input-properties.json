{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema@0.8.0/resources/schema.json",
  "name": "Test",
  "id": "test",
  "version": 1,
  "appliesTo": [
    "bpmn:ServiceTask"
  ],
  "groups": [],
  "properties": [
    {
      "value": "test",
      "label": "Topic",
      "type": "String",
      "editable": false,
      "binding": {
        "type": "zeebe:taskDefinition:type"
      }
    },
    {
      "value": "=name",
      "label": "Input: Property1",
      "type": "Text",
      "binding": {
        "type": "zeebe:input",
        "name": "name"
      }
    },
    {
      "value": "=id",
      "label": "Input: Property2",
      "type": "String",
      "constraints": {
        "notEmpty": false
      },
      "binding": {
        "type": "zeebe:input",
        "name": "id"
      }
    },
    {
      "value": "nameResult",
      "label": "Output: name",
      "type": "String",
      "binding": {
        "type": "zeebe:output",
        "source": "=name"
      }
    },
    {
      "value": "idResult",
      "label": "Output: id",
      "type": "String",
      "binding": {
        "type": "zeebe:output",
        "source": "=id"
      }
    }
  ]
}
